<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>HttpResponseTime</name>
    <probeName>url-${url.signature}</probeName>
    <probeClass>jrds.probe.HttpResponseTime</probeClass>
    <specific name="command">httptest</specific>
    <specific name="arguments">${url}</specific>
    <ds>
        <dsName>Connect</dsName>
        <dsType>gauge</dsType>
    </ds>
    <ds>
        <dsName>First Byte</dsName>
        <dsType>gauge</dsType>
    </ds>
    <ds>
        <dsName>Last Byte</dsName>
        <dsType>gauge</dsType>
    </ds>
    <graphs>
        <name>HttpGraph</name>
    </graphs>
</probedesc>
